{
  "name": "bookie",
  "version": "1.0.0",
  "description": "Reading tracker application built using MERN stack",
  "main": "index.js",
  "type": "module",
  "scripts": {
    "install-server": "npm install",
    "install-dependencies": " npm run install-client && npm run install-server ",
    "build-client": "cd client && npm run build",
    "install-client": "cd client && npm install",
    "heroku-build": "npm run install-client && npm run build-client",
    "server": "nodemon server.js",
    "client": "npm start --prefix client",
    "start": "concurrently \" npm run server \" \" npm run client \" "
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/mohammadkhalaf/bookie.git"
  },
  "author": "Mohammad Khalaf",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/mohammadkhalaf/bookie/issues"
  },
  "homepage": "https://github.com/mohammadkhalaf/bookie#readme",
  "devDependencies": {
    "concurrently": "^7.1.0",
    "nodemon": "^2.0.15"
  },
  "dependencies": {
    "bcryptjs": "^2.4.3",
    "dotenv": "^16.0.0",
    "express": "^4.17.3",
    "express-async-errors": "^3.1.1",
    "express-mongo-sanitize": "^2.2.0",
    "express-rate-limit": "^6.5.1",
    "helmet": "^5.1.1",
    "jsonwebtoken": "^8.5.1",
    "moment": "^2.29.4",
    "mongoose": "^6.3.0",
    "morgan": "^1.10.0",
    "validator": "^13.7.0",
    "xss-clean": "^0.1.1"
  },
  "engines": {
    "node": "18.x"
  }
}
